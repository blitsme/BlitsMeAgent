<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    >
    <!-- Resource dictionary entries should be defined here. -->
    <!--ACCENT COLORS-->
    <Color x:Key="HighlightDarkColor">#FF2B519B</Color>
    <Color x:Key="HighlightLightColor">#FF2B519B</Color>

    <Color x:Key="AccentDarkColor">#FF2B519B</Color>

    <!--80%-->
    <Color x:Key="AccentColor">#CC2B519B</Color>
    <!--60%-->
    <Color x:Key="AccentColor2">#992B519B</Color>
    <!--40%-->
    <Color x:Key="AccentColor3">#662B519B</Color>
    <!--20%-->
    <Color x:Key="AccentColor4">#332B519B</Color>

    <LinearGradientBrush x:Key="BrandingBrush" EndPoint="0.001,0.5" StartPoint="1.002,0.5">
        <GradientStop Color="{DynamicResource HighlightDarkColor}" Offset="0" />
        <GradientStop Color="#FF5FC316" Offset="1" />
    </LinearGradientBrush>

    <SolidColorBrush x:Key="AccentColorBrush" Color="{StaticResource AccentColor}" />

    <SolidColorBrush x:Key="AccentSelectedColorBrush" Color="White" />
    <Color x:Key="HeaderFontColor">#95B3D7</Color>
</ResourceDictionary>